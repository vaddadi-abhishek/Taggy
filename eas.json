{
  "cli": {
    "version": ">= 16.13.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "TWITTER_CLIENT_ID": "prod_id",
        "TWITTER_CLIENT_SECRET": "prod_secret"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
